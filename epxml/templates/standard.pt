<table class="schedule">
    <tbody>
      {% for entry in entries %}
        <tr class="entry" data-entry-id="{{ entry.attrib['id']}}">
            <td class="categories"> {{ entry.category }} </td>
            <td class="audience"> {{ entry.audience }} </td>
            <td class="title"> {{ entry.title }} </td>
            <td class="topics"> 
                {% for topic in  entry.topics %} 
                    <span class="topic">{{ topic.topic }}</span>
                {% endfor %}
            </td>
            <td class="speakers"> 
                {% for speaker in  entry.speakers %} 
                   <div class="speaker" data-speaker-id="{{ speaker.speaker.attrib['id'] }}">
                        <span class="name">{{ speaker.speaker.name }}</span>
                        <span class="profile">{{ speaker.speaker.profile}}</span>
                    </div>
                {% endfor %}
            </td>
        </tr>  
      {% endfor %}
  </tbody>
</table>
